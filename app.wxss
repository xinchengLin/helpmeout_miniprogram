/**app.wxss**/
page{
  height: 100%;
  background-color: #eee;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
   align-items: center; 
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.tipText{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  box-sizing: border-box;    /* 此元素的内边距和边框不再会增加它的宽度 */
  background: #EEE8AA;
  font-size: 30rpx;
  padding: 0 20rpx;
  display: flex;
  justify-content: flex-start;
}

image.tipImage{
  width: 4%;
  height: 30rpx;
  margin: auto 5rpx;
}

.notice{
  width: 95%;
  display: inline-block;
  overflow: hidden;
}

.nullOrder{
  width: 100%;
  height: 100%;
  position: relative;
  top: 20%;
}

.buttonClass{
  padding: 0 3.4rem;
  color: #f06f7f;
  height: 88rpx;
  line-height: 88rpx;
  border: 1px solid #f06f7f;
  background: transparent;
}
.buttonHover{
  background-color: #f06f7f;
  color: #fff !important;
}

.navbg{
  width: 100%;
  height: 80rpx;
  background: #fff;
}
.scroll-view_H{
  width: 100%;
  height: 100%;
  display: flex;
}
.normal{
  flex: 1;
  line-height: 80rpx;
  font-size: 32rpx;
  text-align:center;
}
.select{
  flex: 1;
  line-height: 80rpx;
  color: #f06f7f;
  font-size: 32rpx;
  box-shadow: border-box;
  text-align:center;
  border-bottom: 5rpx solid #f06f7f;
}
scroll-view{
  height: 100%;
}
swiper{
  width: 100%;
  margin-top: 10rpx;
}
swiper-item{
 border-left: 1rpx solid #eee; 
}

@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

