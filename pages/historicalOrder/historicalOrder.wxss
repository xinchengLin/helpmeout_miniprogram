/* pages/historicalOrder/historicalOrder.wxss */

.container {
  padding: 0;
  overflow: hidden;
}

.content {
  width: 100%;
}

.mianBody {
  background-color: #fff;
  font-size: 35rpx;
  padding: 30rpx 20rpx;
  margin-bottom: 20rpx;
}

.modelMess {
  font-size: 33rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.medul {
  width: 100%;
}

.view-item {
  width: 50%;
  display: inline-block;
}

.item-text {
  color: #999;
  font-size: 28rpx;
}

.item-mess {
  display: block;
  font-size: 35rpx;
}

.other-button-hover {
  background-color: rgba(0, 0, 0, 0.1);
  opacity: 0.7;
}

.carMess {
  background-color: #fff;
  margin-bottom: 20rpx;
}

.medul-content, .medul-last {
  width: 33%;
  display: inline-block;
  text-align: center;
}

.medul .medul-heading {
  width: 39%;
  margin-left: 0;
  display: inline-block;
  text-align: center;
}

.medul-address {
  width: 33%;
  display: inline-block;
  text-align: center;
}

.medul-address, .medul-content, .medul-heading, .medul-last {
  margin-top: 10rpx;
  height: 60rpx;
}

.medul-address, .medul-content, .medul-last {
  font-size: 28rpx;
}

.medul-address {
  height: 70rpx;
}

.itemTitle {
  width: 50%;
  text-align: right;
}

.itemContent {
  width: 50%;
  text-align: left;
}

/*图标字体*/

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 35rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-jiage:before {
  content: "\e66a";
}

.icon-xingmingyonghumingnicheng:before {
  content: "\e623";
}

.icon-diandongche:before {
  content: "\e602";
}

.icon-icon-:before {
  content: "\e6e5";
}

.icon-weibiaoti-:before {
  content: "\e60d";
}

.nullOrder {
  position: relative;
  top: 20%;
  height: 30vh;
  padding-top: 115rpx;
  left: 15%;
  width: 70%;
}

.head {
  padding-left: 60rpx;
}

.buttonClass1 {
  height: 65rpx;
}

.medul-heading-1 {
  text-align: right !important;
  margin-left: 35rpx !important;
}

.finally {
  position: relative;
  overflow: hidden;
}

.img {
  width: 165rpx;
  height: 165rpx;
  position: absolute;
  right: 15rpx;
  top: -19rpx;
  z-index: 1001;
}
.inputMess{
  margin-top:20rpx; 
  width: 100%;
  height: 150rpx;
  font-size: 30rpx;
}
.bai{
  overflow: hidden;
}
