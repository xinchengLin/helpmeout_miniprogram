page {
  display: flex;
  flex-direction: column;
  height: 100%;
  background-color: #eee;
}

picker {
  text-align: center;
  color: #515151;
}

picker view image {
  width: 30rpx;
  height: 30rpx;
  position: relative;
  top: 4rpx;
}

.navbar {
  flex: none;
  display: flex;
  background: #fff;
  font-size: 32rpx;
}

.navbar .item {
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 80rpx;
}

.navbar .item.active {
  color: #f06f7f;
}

.navbar .item.active:after {
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 4rpx;
  background: #f06f7f;
}

.hd {
  font-size: 32rpx;
}

.medul {
  width: 100%;
}

.view-item {
  width: 50%;
  display: inline-block;
}

.item-text {
  color: #999;
  font-size: 28rpx;
}

.item-mess {
  display: block;
  font-size: 35rpx;
}

.other-button-hover {
  background-color: rgba(0, 0, 0, 0.1);
  opacity: 0.7;
}

.carMess {
  background-color: #fff;
  margin-top: 20rpx;
}

.tipText {
  width: 100%;
  box-sizing: border-box; /* 此元素的内边距和边框不再会增加它的宽度 */
  background: #eee8aa;
  padding: 20rpx;
  font-size: 30rpx;
  text-align: center;
}

.medul-content, .medul-last {
  width: 33%;
  display: inline-block;
  text-align: center;
}

.medul .medul-heading {
  width: 39%;
  margin-left: 0;
  display: inline-block;
  text-align: center;
}

.medul-address {
  width: 33%;
  display: inline-block;
  text-align: center;
}

.medul-address, .medul-content, .medul-heading, .medul-last {
  margin-top: 10rpx;
  height: 60rpx;
}

.medul-address, .medul-content, .medul-last {
  font-size: 28rpx;
}

.medul-address {
  height: 70rpx;
}

.itemTitle {
  width: 50%;
  text-align: right;
}

.itemContent {
  width: 50%;
  text-align: left;
}

.phoneNum {
  color: #09f;
  text-decoration: underline;
  display: inline-block;
}

/*图标字体*/

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 35rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-jiage:before {
  content: "\e66a";
}

.icon-xingmingyonghumingnicheng:before {
  content: "\e623";
}

.icon-diandongche:before {
  content: "\e602";
}

.icon-icon-:before {
  content: "\e6e5";
}

.icon-weibiaoti-:before {
  content: "\e60d";
}

.nullOrder {
  position: relative;
  top: 20%;
  left: 15%;
  height: 30vh;
  padding-top: 300rpx;
  width: 70%;
}

.head {
  padding-left: 60rpx;
}

.buttonClass1 {
  height: 65rpx;
}

.buttonClass2 {
  display: block;
  width: 60%;
  margin-bottom: 20rpx;
  padding: 0 3.3rem;
}

.buttonClass3 {
  height: 65rpx;
  background: #eee;
}

.medul-heading-1 {
  text-align: right !important;
  margin-left: 35rpx !important;
}

.detail {
  margin-top: 25rpx;
}
